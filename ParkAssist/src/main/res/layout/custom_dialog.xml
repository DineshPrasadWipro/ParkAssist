<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <alliancex.renault.ui.RenaultTextView
        android:id="@+id/apa_label"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        style="@style/RenaultTextView.Body1.Compressed"
        android:gravity="center"
        android:maxLines="2"
        android:layout_marginLeft="@dimen/custom_dialog_horizontal_margin"
        android:layout_marginRight="@dimen/custom_dialog_horizontal_margin"
        android:layout_marginVertical="@dimen/custom_dialog_vertical_margin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>