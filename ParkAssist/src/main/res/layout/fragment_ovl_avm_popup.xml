<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/camera_ovl_avm_popup_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="@color/tools_purple_alpha_50">

    <tag
        android:id="@id/uxTag"
        android:value="" />

    <fragment
        android:id="@+id/camera_sonar_mute_fragment"
        android:name="com.renault.parkassist.ui.sonar.SonarMuteFragment"
        android:layout_width="@dimen/rd_icon_size"
        android:layout_height="@dimen/rd_icon_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="@dimen/avm_popup_mute_vertical_bias"
        tools:layout="@layout/sonar_mute" />

</androidx.constraintlayout.widget.ConstraintLayout>