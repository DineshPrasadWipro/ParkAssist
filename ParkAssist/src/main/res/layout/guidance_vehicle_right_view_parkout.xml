<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_vehicle_parallel_left_front"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_height="@dimen/apa_guidance_vehicle_center_view_height_parkout"
    tools:layout_width="@dimen/apa_guidance_vehicle_center_view_width_parkout">

    <ImageView
        android:id="@+id/right_easypark_front"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/apa_guidance_vehicle_right_easypark_front_start_parkout"
        android:layout_marginTop="@dimen/apa_guidance_vehicle_right_easypark_front_top_parkout"
        android:layout_marginRight="@dimen/apa_guidance_vehicle_right_easypark_front_end_parkout"
        android:layout_marginBottom="@dimen/apa_guidance_vehicle_right_easypark_front_bottom_parkout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ric_adas_park_assist_easypark"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/right_car"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/apa_guidance_vehicle_right_car_start_parkout"
        android:layout_marginTop="@dimen/apa_guidance_vehicle_right_car_top_parkout"
        android:layout_marginRight="@dimen/apa_guidance_vehicle_right_car_end_parkout"
        android:layout_marginBottom="@dimen/apa_guidance_vehicle_right_car_bottom_parkout"
        android:rotation="@dimen/apa_guidance_vehicle_right_car_rotation_right"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rimg_exter_vehicle_top_01"
        tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>
