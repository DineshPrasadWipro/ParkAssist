<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_dark"
    tools:background="@drawable/ic_grid_port"
    tools:layout_height="@dimen/apa_scanning_view_height"
    tools:layout_width="@dimen/apa_scanning_view_width">

    <ImageView
        android:id="@+id/map_area"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@drawable/rimg_bckg_apa_scanning_perpendicular_left" />

    <com.renault.ui.apascanning.ApaScanningTopCarView
        android:id="@+id/hfp_scanning_car_top_view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/apa_scanning_car_top_view_start"
        android:layout_marginTop="@dimen/apa_scanning_car_top_view_top"
        android:layout_marginRight="@dimen/apa_scanning_car_top_view_end"
        android:layout_marginBottom="@dimen/apa_scanning_car_top_view_bottom"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/right_parking_slot_parallel"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/apa_scanning_right_parking_parallel_start"
        android:layout_marginTop="@dimen/apa_scanning_right_parking_parallel_top"
        android:layout_marginRight="@dimen/apa_scanning_right_parking_parallel_end"
        android:layout_marginBottom="@dimen/apa_scanning_right_parking_parallel_bottom"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rimg_adas_parking"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/left_parking_slot_parallel"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/apa_scanning_left_parking_parallel_start"
        android:layout_marginTop="@dimen/apa_scanning_left_parking_parallel_top"
        android:layout_marginRight="@dimen/apa_scanning_left_parking_parallel_end"
        android:layout_marginBottom="@dimen/apa_scanning_left_parking_parallel_bottom"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rimg_adas_parking"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/left_parking_slot_perpendicular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/apa_scanning_left_parking_perpendicular_start"
        android:layout_marginTop="@dimen/apa_scanning_left_parking_perpendicular_top"
        android:layout_marginRight="@dimen/apa_scanning_left_parking_perpendicular_end"
        android:layout_marginBottom="@dimen/apa_scanning_left_parking_perpendicular_bottom"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rimg_adas_parking"
        tools:visibility="visible" />

    <ImageView
        android:id="@+id/right_parking_slot_perpendicular"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="@dimen/apa_scanning_right_parking_perpendicular_start"
        android:layout_marginTop="@dimen/apa_scanning_right_parking_perpendicular_top"
        android:layout_marginRight="@dimen/apa_scanning_right_parking_perpendicular_end"
        android:layout_marginBottom="@dimen/apa_scanning_right_parking_perpendicular_bottom"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rimg_adas_parking"
        tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>