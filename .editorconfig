[*]
charset = utf-8
end_of_line = lf
insert_final_newline = false
indent_style = space
indent_size = 4
max_line_length = 100



[*.json]
indent_style = space
indent_size = 2

[*.json5]
indent_style = space
indent_size = 2

[{*.yml, *.yaml}]
indent_style = space
indent_size = 2

[*.kt]
disabled_rules=no-wildcard-imports